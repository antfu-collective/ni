{
  "name": "@antfu/ni",
  "version": "0.19.0",
  "packageManager": "pnpm@7.4.0",
  "description": "Use the right package manager",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/antfu/ni#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/ni.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/ni/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "ni": "bin/ni.mjs",
    "nci": "bin/nci.mjs",
    "nr": "bin/nr.mjs",
    "nu": "bin/nu.mjs",
    "nix": "bin/nix.mjs",
    "na": "bin/na.mjs",
    "nun": "bin/nun.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "esno src/commands/ni.ts",
    "nr": "esno src/commands/nr.ts",
    "build": "unbuild",
    "stub": "unbuild --stub",
    "release": "bumpp && npm publish",
    "lint": "eslint .",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@types/ini": "^1.3.31",
    "@types/node": "^18.11.18",
    "@types/prompts": "^2.4.2",
    "@types/which": "^2.0.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.32.0",
    "esno": "^0.16.3",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.12",
    "find-up": "^6.3.0",
    "fzf": "^0.5.1",
    "ini": "^3.0.1",
    "kleur": "^4.1.5",
    "prompts": "^2.4.2",
    "rimraf": "^4.1.1",
    "terminal-link": "^3.0.0",
    "typescript": "^4.9.4",
    "unbuild": "^1.1.1",
    "vitest": "^0.27.2",
    "which": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
